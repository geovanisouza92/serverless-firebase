{
  "name": "serverless-firebase",
  "version": "1.0.0",
  "scripts": {
    "install": "yarn build-all",
    "dev": "yarn build-firebase && yarn && yarn next",
    "preserve": "yarn build-all",
    "serve": "firebase serve",
    "predeploy": "yarn build-all",
    "deploy": "firebase deploy",
    "build-all": "yarn build-next && yarn build-firebase",
    "build-next": "next build",
    "build-firebase": "cd \"functions\" && yarn"
  },
  "license": "MIT",
  "dependencies": {
    "firebase": "^4.1.3",
    "next": "^2.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
